Signed-off-by: Koen Kooi <koen.kooi@linaro.org>
---
 plat/hikey/include/usb.h | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/plat/hikey/include/usb.h b/plat/hikey/include/usb.h
index 8fdcc6c..00526bd 100644
--- a/plat/hikey/include/usb.h
+++ b/plat/hikey/include/usb.h
@@ -776,7 +776,7 @@ struct usb_endpoint_descriptor {
         unsigned char  bmAttributes;
         unsigned short wMaxPacketSize;
         unsigned char  bInterval;
-} __attribute__ ((packed));
+};
 
 #define USB_DT_ENDPOINT_SIZE            7
 #define USB_DT_ENDPOINT_AUDIO_SIZE      9       /* Audio extension */
